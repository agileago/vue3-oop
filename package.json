{
  "name": "vue3-oop",
  "version": "0.5.0",
  "main": "dist/vue3-oop.cjs.js",
  "module": "dist/vue3-oop.es.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-oop.es.js",
      "require": "./dist/vue3-oop.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && rimraf types && tsc --build tsconfig.build.json",
    "serve": "vite preview",
    "type:check": "tsc --noEmit --skipLibCheck",
    "commit": "cz-customizable",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "release": "pnpm build && release-it",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest --ui",
    "coverage": "vitest --coverage"
  },
  "files": [
    "dist",
    "types",
    "package.json"
  ],
  "author": {
    "name": "agileago",
    "email": "593728759@qq.com"
  },
  "keywords": [
    "vue-oop",
    "oop",
    "vue",
    "di",
    "vue-di",
    "ioc",
    "vue class component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agileago/vue3-oop.git"
  },
  "homepage": "https://agileago.github.io/vue3-oop",
  "license": "MIT",
  "devDependencies": {
    "@abraham/reflection": "^0.9.1",
    "@commitlint/cli": "^16.2.1",
    "@nexhome/yorkie": "^2.0.8",
    "@release-it/conventional-changelog": "^4.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitest/ui": "^0.5.9",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue3-oop/plugin-vue-jsx": "^1.4.0",
    "ant-design-vue": "^3.0.0-beta.8",
    "autobind-decorator": "^2.4.0",
    "c8": "^7.11.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "happy-dom": "^2.45.0",
    "injection-js": "^2.4.0",
    "lint-staged": "^12.3.4",
    "lodash-es": "^4.17.21",
    "prettier": "^2.5.1",
    "release-it": "^14.12.5",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitepress": "^0.22.3",
    "vitest": "^0.5.9",
    "vue": "^3.2.31",
    "vue-router": "^4.0.13"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "peerDependencies": {
    "@abraham/reflection": "*",
    "injection-js": "^2.4.0",
    "vue": "^3.2.20"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "ignoreRecommendedBump": true
      }
    }
  }
}
